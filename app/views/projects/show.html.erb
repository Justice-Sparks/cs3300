
<section class="bg-warning pt-5 pb-5">
<div class="container-fluid">
  <div class="show">
    <p id="notice"><%= notice %></p>

    <p>
      <h3> Project - Title:</h3>
      <%= @project.title %>
    </p>

    <p>
      <h3>Project - Description:</h3>
      <%= @project.description %>
    </p>

    <%= link_to 'Edit', edit_project_path(@project), class: "btn btn-success" %> |
    <%= link_to 'Back', projects_path , class: "btn btn-success" %>
  </div>
</div>
</section>
